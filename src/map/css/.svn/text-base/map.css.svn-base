/* file: style.css */
/************************************************************************
    Start of myc-map
************************************************************************/
#myc-map {
    position:           relative;
    z-index:            1;
}
/************************************************************************
    End of myc-map
************************************************************************/

/************************************************************************
    Start of ymap
************************************************************************/
.map{
    height:             100%;
    width:              100%;
}
/************************************************************************
    Overwrite ymaps sprite
************************************************************************/
html .yui3-map .yui3-map-content .ymap-viewport .ymaps-sprite,
html .yui3-controlmaptype-content .header .maptype-dropdown-hint,
html .kb-overlay-holder .kb-head .close {
    background-image:   url(../../app/img/ymaps.png);
    background-repeat:  no-repeat;
    background-color:   transparent;
}
/************************************************************************
    End of Overwrite ymaps sprite
************************************************************************/

/************************************************************************
    Start of normal pan controls
************************************************************************/
.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan-body {
    background-position:-1px -2750px;
    width:              49px;
    height:             52px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan-body.up-hover {
    background-position:-1px -4350px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan-body.down-hover {
    background-position:-1px -4409px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan-body.left-hover {
    background-position:-1px -4531px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan-body.right-hover {
    background-position:-1px -4467px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan button.up {
    background:         none repeat scroll 0 0 transparent;
    left:               17px;
    top:                4px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan button.left {
    background:         none repeat scroll 0 0 transparent;
    left:               3px;
    top:                18px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan button.down {
    background:         none repeat scroll 0 0 transparent;
    left:               16px;
    top:                32px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlpan button.right {
    background:         none repeat scroll 0 0 transparent;
    left:               32px;
    top:                18px;
}
/************************************************************************
    End of normal pan controls
************************************************************************/

/************************************************************************
    Start of normal zoom controls
************************************************************************/
.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoomincontainer.ymaps-sprite,
.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoomoutcontainer.ymaps-sprite {
    background:         none;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlzoom-content .yui3-slider-y .yui3-slider-rail {
    left:               8px;
    top:                29px;
    background-position:-36px -4185px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlzoom-content .yui3-slider-y .yui3-slider-thumb-image {
    background-position:1px -4104px;
    left:               -2px;
    box-shadow:         none;
    z-index:            1;
}

.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoominbutton.ymaps-sprite {
    background-position:-50px -951px;
    width:              17px;
    height:             26px;
    left:               7px;
    top:                6px;
}

.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoominbutton.ymaps-sprite:hover {
    background-position:0 -951px;
    left:               6px; 
    width:              19px;
}

.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoomoutbutton.ymaps-sprite {
    background-position:-50px -998px;
    top:                -1px;
    width:              17px;
    height:             23px;
    left:               7px;
}

.yui3-map .yui3-map-content .ymap-viewport .ymap-control-zoomoutbutton.ymaps-sprite:hover {
    background-position:0 -998px;
    left:               6px;
    width:              19px;
}

.yui3-map .yui3-controlzoom-content .yui3-skin-sam .yui3-slider-y .yui3-slider-thumb {
    height:             10px;
    width:              30px;
}
/************************************************************************
    End of normal zoom controls
************************************************************************/

/************************************************************************
    start of simple small zoom controls
************************************************************************/
.yui3-map .yui3-map-content .ymap-viewport .yui3-controlsmallsimplezoom-content{
    background: transparent;
    border: none;
    box-shadow: none;
    padding: 0;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlsmallsimplezoom-content .in {
    background-position: -3px -4126px;
    width:              21px;
    height:             22px;
    margin-top:         0;  
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlsmallsimplezoom-content .out {
    background-position: -3px -4156px;
    width:              21px;
    height:             23px;
    margin-top:         0px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlsmallsimplezoom-content .in:hover {
    background-position: -30px -4123px;
    width:              21px;
    height:             25px;
    margin-left:        -1px;
    margin-top:         -3px;
}

.yui3-map .yui3-map-content .ymap-viewport .yui3-controlsmallsimplezoom-content .out:hover {
    background-position: -30px -4156px;
    width:              21px;
    margin-left:        -1px;
}
/************************************************************************
    End of simple small zoom controls
************************************************************************/
/* HACK: Forcibly move on-map logos above our footer.*/
html.onebox-main .ymap-controlsBody .yui3-controlbranding {
    margin-bottom:          2em;
}

html.onebox-main .ymap-controlsBody .yui3-controlcopyright {
    margin-bottom:          2.5em;
}
/************************************************************************
    End of ymap
************************************************************************/

/* file: controls.css */

/************************************************************************
    Start of myc-map map type and traffic controls
************************************************************************/
.map .yui3-controltraffic,
.map .yui3-controlmaptype-content .header {
    color:              rgba(0,87,144,1);
    text-shadow:        1px 1px 0px rgba(255,255,255,0.8);
    border:             1px solid #dadee6;
    background:         #f5f7f9; /* Old browsers */
    background:         -moz-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* FF3.6+ */
    background:         -webkit-gradient(linear, left top, left bottom, color-stop(30%, #ffffff), color-stop(100%, #f5f7f9)); /* Chrome,Safari4+ */
    background:         -webkit-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* Chrome10+,Safari5.1+ */
    background:         -o-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* Opera11.10+ */
    background:         -ms-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* IE10+ */
    filter:             progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f5f7f9', GradientType=0 ); /* IE6-9 */
    background:         linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* W3C */
    opacity:            1;
}

.map .yui3-controltraffic,
.map .yui3-controltraffic-clicked,
.map .yui3-controlmaptype-content .header {
    -moz-border-radius:  0;
    -webkit-border-radius: 0;
    border-radius:      0;
}

.map .yui3-controlmaptype-content .header {
    border:             none;
    -moz-box-shadow:    none;
    -webkit-box-shadow: none;
    box-shadow:         none;
}

.map .yui3-controltraffic {
    margin-right:       9px;
    border:             1px solid #a4a4a3;
    -moz-box-shadow:    1px 1px 3px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
    box-shadow:         1px 1px 3px rgba(0, 0, 0, 0.2);
}

.map .yui3-controlmaptype-content .maptype-dropdown-hidden .spacer,
.map .yui3-controlmaptype-content .header a,
.map .yui3-controlmaptype-content .header a:hover,
.map .yui3-controlmaptype-content .header a:visited,
.map .yui3-controlmaptype-content .maptype-dropdown a,
.map .yui3-controlmaptype-content .maptype-dropdown a:hover,
.map .yui3-controlmaptype-content .maptype-dropdown a:visited {
    color:              #1D1DA3;
}

.map .yui3-controltraffic a.css3-button,
.map .yui3-controltraffic a.css3-button:hover,
.map .yui3-controltraffic a.css3-button:active,
.map .yui3-controltraffic a.css3-button:visited {
    color:              #1D1DA3;
    line-height:        12px;
    padding:            5px 9px;
    text-shadow:        1px 1px 0px rgba(255,255,255,0.8);
    text-decoration:    none;
}

.map .yui3-controltraffic a.css3-button:hover,
.map .yui3-controlmaptype-content .maptype-dropdown li:hover {
    background:         rgba(240,240,240,1);
    text-decoration:    none;
    -moz-box-shadow:    0px 0px 2px rgba(0,0,0,0.4);
    -webkit-box-shadow: 0px 0px 2px rgba(0,0,0,0.4);
}

.map .yui3-controltraffic-clicked {
    border:             1px solid #d2d6dc;
    background-color:   #415575;
    background:         -moz-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background:         -webkit-gradient(linear,left top,left bottom,color-stop(0%,rgba(66,115,169,0.95)),color-stop(100%,rgba(65,85,117,0.95)));
    background:         -webkit-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background:         -o-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background:         -ms-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background:         linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    opacity:            1;
}

.map .yui3-controltraffic a.css3-button-clicked,
.map .yui3-controltraffic a.css3-button-clicked:hover,
.map .yui3-controltraffic a.css3-button-clicked:active,
.map .yui3-controltraffic a.css3-button-clicked:visited {
    color:              #fff;
    line-height:        12px;
    padding:            5px 9px;
    text-decoration:    none;
    background-color:   #415575;
    background: -moz-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background: -webkit-gradient(linear,left top,left bottom,color-stop(0%,rgba(66,115,169,0.95)),color-stop(100%,rgba(65,85,117,0.95)));
    background: -webkit-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background: -o-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background: -ms-linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    background: linear-gradient(top,rgba(66,115,169,0.95) 0,rgba(65,85,117,0.95) 100%);
    text-shadow:        none;
}

.map .yui3-controlmaptype-content .header {
    cursor:             default;
    padding:            5px 18px 5px 9px;
    width:              43px;
}

.map .yui3-controlmaptype-content .header a {
    line-height:        12px;
}

.map .yui3-controlmaptype-content {
    opacity:            1;
    border:             1px solid #a4a4a3;
    -moz-box-shadow:    1px 1px 3px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
    box-shadow:         1px 1px 3px rgba(0, 0, 0, 0.2);
}

.map .yui3-controlmaptype-content .header .maptype-dropdown-hint {
    cursor:             pointer;
    right:              5px;
}

.map .yui3-controlmaptype-content .maptype-dropdown li,
.map .yui3-controlmaptype-content .maptype-dropdown-hidden li {
    border-bottom:      1px solid #dadee6;
    border-left:        1px solid #dadee6;
    border-right:       1px solid #dadee6;
    padding:            5px 9px;
}
/************************************************************************
    End of myc-map map type and traffic controls
************************************************************************/

/* YUI CSS Detection Stamp */
#yui3-css-stamp.myc-map-css { display: none; }