.business-container{
    padding:            0 0 0.769em;
}

.businesses .title{
    width:              82%;
}

.businesses h4{
    font-weight:        bold;
    font-size:          1.231em;
    line-height:        17px;
    padding:            0;   
}

.businesses .more-info{
    font-size:          0.75em;
    line-height:        16px;
}

.businesses li.business{
    position:           relative;
    padding:            1.1538em 0.769em;
}

.businesses li.business:hover{
    background:             #FBFBFF;
}

.businesses li.business.last-child{
    margin-bottom:      20px;
}

.businesses .icon {
    cursor:             pointer;
    width:              2.231em;
}

.businesses .details {
    width:              90%;
    cursor:             pointer;
}

.businesses .distance{
    position:           absolute;
    top:                15px;
    right:              10px;
}

.businesses .businessMap .distance{
    display:            none;
}

.businesses p{
    font-size:          1em;
    line-height:        17px;
}

.businesses .rating-review {
    padding:            3px 0 0 0;
}

.businesses .rating-review a{
    color:              #5F5F5F;
    padding-left:       0.364em;
    font-size:          0.8459em;
    line-height:        14px;
    vertical-align:     top;
}

.businesses .web a{
    color:              #1E7D83;
}

.businesses .closed_business{
    background:         url(../../app/img/ymaps.png) no-repeat 0 -3997px;
    padding-left:       21px;
    padding-top:        1px;
}

.businesses .closed_business span {
    color:              #CC0000;
}

.businesses ul.actions {
    padding-left:       2.308em;
}

.businesses .pagination,
.businesses .hiConf-link,
.businesses .seeMore-link{
    padding:           0 0.769em 0.923em;
}

.businesses .hiConf-link{
    font-weight:        bold;
}

.businesses .seeMore-link{
    font-weight:        bold;
}

.tel span,
.url span,
span.review {
    left:               -9999em;
    position:           absolute;
}

/************************************************************************
    Start of share
************************************************************************/

.businesses .share-form .s-close {
    top:                8px;
    right:              8px;
}

.businesses .share-form .link-container {
    margin:             0;
}

.businesses .share-form input[type="text"]{
    width:              97.5%;
    margin:             10px 0;
}

.businesses .share-form .social-container{
    margin:             0;
}

.businesses .share-form .social-container .facebook-share{
    width:              92px;
    overflow:           hidden;
    heigth:             20px;
}

/************************************************************************
    End of share
************************************************************************/

/************************************************************************
    Not Found
************************************************************************/

.businesses .not-found{
    padding:            1.538em 0.769em 0.615em;
}

.businesses .not-found h4 {
    font-weight:        normal;
    color:              #cc0000;
    font-size:          1.077em;
    margin:             0 0 22px 0;
    padding:            0px;
    line-height:        17px;
}

.businesses .not-found h5 {
    font-weight:        bold;
    font-size:          1.077em;
    margin:             0 0 8px 0;
    padding:            0px;
    line-height:        17px;
}

.businesses .not-found ul,
.businesses .not-found li {
    list-style:         disc outside none;
}

.businesses .not-found ul{
    font-size:          0.923em;
    padding:            0 0 0 15px;
}

.businesses .not-found ul li{
    padding:            0 0 8px 0;
}

/************************************************************************
    End Not Found
************************************************************************/

/* YUI CSS Detection Stamp */
#yui3-css-stamp.myc-businesses-css { display: none; }