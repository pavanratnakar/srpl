.icon {
    color:                  #fff;
}

.icon .i_left{
    width:                  4px;
    height:                 21px;
    float:                  left;
    display:                inline;
}

.icon .i_center {
    width:                  13px;
    height:                 31px;
    text-align:             center;
    font-size:              11px;
    line-height:            21px;
    font-weight:            bold;
    float:                  left;
    display:                inline;
    position:               relative;
}

.icon .i_right{
    width:                  5px;
    height:                 21px;
    float:                  left;
    display:                inline;
}

.icon .star,
.yui3-myc-marker .yui3-marker-content .star {
    background:             url(../../app/img/myc.png) no-repeat 0 -744px;
    width:                  15px;
    height:                 14px;
    position:               absolute;
    top:                    2px;
    left:                   -1px;
}

