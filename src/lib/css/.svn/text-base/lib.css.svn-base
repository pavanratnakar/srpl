/************************************************************************
    Start of pagination.css
************************************************************************/

.pagination{
    text-align:     center;
}

.pagination ul{
    margin:         12px 0 0 0;
}

.pagination ul li{
    display:        inline-block;
    padding:        0;
    zoom:           1;
    *display:       inline;
    letter-spacing: normal;
}

.pagination button{
    padding:        0;
    color:          #324FE1;
    font-size:      13px;
}

.pagination button.paginate {
    height:         24px;
    width:          30px;
    border:         1px solid #FFF;
}

.pagination button.paginate:hover,
.pagination button.paginate.active {
    border:         1px solid #E2E2E6;
    background:     #FAFAFC;
    text-decoration: none;
}

.pagination button.paginate.active {
    font-weight:    bold;
    color:          #3F3F3F;
}

/************************************************************************
    End of pagination.css
************************************************************************/

/************************************************************************
    Start of rating.css
************************************************************************/
.rating-static {
    width:      75px;
    height:     14px;
    display:    block;
    background: url(../../app/img/myc.png) 0 0 no-repeat;
    line-height:0;
    font-size:  0;
}
.rating-50 { background-position: 0 -1187px; }
.rating-40 { background-position: 0 -1163px; } 
.rating-30 { background-position: 0 -1138px; }
.rating-20 { background-position: 0 -1114px; }
.rating-10 { background-position: 0 -1088px; }
.rating-0 { background-position: 0 -1064px; }
 
.rating-5  { background-position: 0 -1036px; }
.rating-15 { background-position: 0 -1009px; }
.rating-25 { background-position: 0 -982px; }
.rating-35 { background-position: 0 -955px; }
.rating-45 { background-position: 0 -926px; }
