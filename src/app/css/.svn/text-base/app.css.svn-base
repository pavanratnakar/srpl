/************************************************************************
    Global Properties
************************************************************************/

html {
    overflow:           hidden;
    height:             100%;
}

body.yui3-skin-sam{
    font:               13px/1.231 "Helvetica Neue",Helvetica,Arial,sans-serif;
    color:              #5F5F5F;
    background-color:   #F5F7F9;
    height:             100%;
    margin:             0;
    padding:            0;
}

h1, h2, h3, h4, h5, h6 {
    color:              #3F3F3F;
}

header, section, footer{
    display:            block;
}

a {
    text-decoration:    none;
    color:              #1D1DA3;
}

a:hover,
a:focus,
.link:hover,
.link:focus {
    text-decoration:    underline;
    outline:            none;
}

body.yui3-skin-sam select, body.yui3-skin-sam input, body.yui3-skin-sam button, body.yui3-skin-sam textarea {
    font-family:        "Helvetica Neue",Helvetica,Arial,sans-serif;
}

.close{
    cursor:             pointer;
}

.bold {
    font-weight:        bold;
}

.f-left{
    float:              left;
    display:            inline;
}

.f-right{
    float:              right;
    display:            inline;
}

.clear{
    clear:              both;
}

.wrap {
    white-space:        pre;           /* CSS 2.0 */
    white-space:        pre-wrap;      /* CSS 2.1 */
    white-space:        pre-line;      /* CSS 3.0 */
    white-space:        -pre-wrap;     /* Opera 4-6 */
    white-space:        -o-pre-wrap;   /* Opera 7 */
    white-space:        -moz-pre-wrap; /* Mozilla */
    word-wrap:          break-word;    /* IE 5+ */
}

.no-focus:focus{
    outline:            none; /* remove default focus border */
}

.link{
    cursor:             pointer;
}

#wrapper{
    margin:             0 auto;
    min-height:         100%;
}

/************************************************************************
    Clear Fix
************************************************************************/

.clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
             
.clearfix {
    display: inline-block;
}
             
html[xmlns] .clearfix {
    display: block;
}
             
* html .clearfix {
    height: 1%;
}

/************************************************************************
    End of Clear Fix
************************************************************************/

/************************************************************************
    End of Global Properties
************************************************************************/

/************************************************************************
    Map Main Container
************************************************************************/

#myc-main-container{
    position:           relative;
}

/************************************************************************
    End of Map Main Container
************************************************************************/

/************************************************************************
    Footer Container
************************************************************************/

#footer {
    background:             #fff;
    background:             rgba( 255, 255, 255, 0.8 );
    border:                 1px solid transparent;
    border-width:           1px 1px 0 0;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
    bottom:                 0;
    font-size:              77%;
    padding:                3px;
    position:               absolute;
    right:                  0;
    z-index:                4;
}
#footer ul {
    display:                inline;
}
#footer li {
    display:                inline;
    margin:                 1px 3px;
}

/************************************************************************
    End of Footer Container
************************************************************************/

/************************************************************************
    Start of sidebar control panel
************************************************************************/

.sidebar-controlpanel .main-options,
.sidebar-controlpanel .tools{
    z-index:            101;
}

.sidebar-controlpanel button:focus{
    outline:            none;
}

.sidebar-controlpanel .main-options{
    position:           absolute;
    top:                10px;
    left:               10px;
}

.sidebar-controlpanel .main-options button.directions-option{
    padding:            10px 20px;
    font-size:          1em;
    line-height:        1em;
    height:             35px;
    width:              131px;
}

.sidebar-controlpanel .tools {
    position:           absolute;
    top:                10px;
    left:               150px;
    height:             35px;
    width:              61px;
}

.sidebar-controlpanel .tools li{
    border:             1px solid #a4a4a3;
    background:         #f5f7f9; /* Old browsers */
    background:         -moz-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* FF3.6+ */
    background:         -webkit-gradient(linear, left top, left bottom, color-stop(30%, #ffffff), color-stop(100%, #f5f7f9)); /* Chrome,Safari4+ */
    background:         -webkit-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* Chrome10+,Safari5.1+ */
    background:         -o-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* Opera11.10+ */
    background:         -ms-linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* IE10+ */
    filter:             progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f5f7f9', GradientType=0 ); /* IE6-9 */
    background:         linear-gradient(top, #ffffff 30%, #f5f7f9 100%); /* W3C */
    -moz-box-shadow:    1px 1px 3px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
    box-shadow:         1px 1px 3px rgba(0, 0, 0, 0.2);
}

.sidebar-controlpanel .tools button{
    padding:            0;
    position:           relative;
    height:             33px;
    width:              29px;
    border:             0;
}

.sidebar-controlpanel .tools button i{
    width:              17px;
    height:             18px;
    display:            block;
    top:                7px;
    left:               6px;
    position:           absolute;
}

.sidebar-controlpanel .divider{
    border-right:       1px solid #a4a4a3;
    height:             21px;
    margin:             6px 0;
}

.sidebar-controlpanel button.print i{
    background:         url(../img/myc.png) no-repeat scroll 0 -198px transparent;
}

.sidebar-controlpanel button.share i{
    background:         url(../img/myc.png) no-repeat scroll 0 -232px transparent;
}

/************************************************************************
    End of sidebar control panel
************************************************************************/

/************************************************************************
    Start of sidebar content
************************************************************************/

.sidebar-notification {
    border-bottom:      1px solid #a4a4a3;
    position:           relative;
}

.sidebar-notification.no-border {
    border:             0;
}

.sidebar-notification .sidebar-title {
    padding:            0.769em 2.616em;
}

.sidebar-notification h3 {
    font-size:          1.077em;
    line-height:        1.077em;
    padding:            0 0 5px;
}

.sidebar-notification h5 {
    font-size:          0.769em;
    line-height:        0.769em;
}

.sidebar-notification .sidebar-toggle button.image{
    position:           absolute;
    bottom:             10px;
    right:              10px;
    width:              18px;
    height:             18px;
    background:         url(../img/myc.png) no-repeat;
    background-position: 0 -2021px;
}

.sidebar-notification .sidebar-toggle button.image:hover{
    background-position: 0 -2077px;
}

.sidebar-notification .sidebar-toggle.minimize button.image{
    background-position: 0 -2049px;
}

.sidebar-notification .sidebar-toggle.minimize button.image:hover{
    background-position: 0 -2105px;
}

/************************************************************************
    Start of sidebar notification
************************************************************************/

/************************************************************************
    End of sidebar notification
************************************************************************/

.sidebar-close {
    position:           absolute;
    right:              10px;
    top:                10px;
}

.sidebar-content {
    position:           absolute;
    left:               10px;
    top:                55px;
    z-index:            101;
    border-color:       #a4a4a3;
    border-width:       1px;
    border-style:       solid;
    -moz-box-shadow:    0px 1px 3px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
    box-shadow:         0px 1px 3px rgba(0, 0, 0, 0.2);
}

.sidebar-content .container{
    width:              26.923em;
    position:           relative;
    background-color:   #FFF;
}

.sidebar-content .main-content{
    width:              100%;
    overflow-y:         auto;
    overflow-x:         hidden;
    position:           relative;
}

.sidebar-content .main-content .main-content-container{
    height:             auto;
    overflow:           hidden;
}

.sidebar-content .main-content.nooverflow{
    overflow:           hidden;
}

.sidebar-content .sidebar-widget{
    display:            block;
    width:              100%;
}

/************************************************************************
    End of sidebar conmtent
************************************************************************/

/************************************************************************
   Overlay popup
************************************************************************/

.popup.overlay {
    font-size:          0.923em;
    color:              #231f20;
    padding:            15px;
    background-color:   #fff;
    border-color:       #d3d3d3;
    border-width:       1px;
    border-style:       solid;
    -moz-box-shadow:    1px 1px 3px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
    box-shadow:         1px 1px 3px rgba(0, 0, 0, 0.2);
    position:           relative;
    max-width:          300px;
}

.popup.overlay p {
    padding:            0 0 20px 0;
}

.popup.overlay ul{
    list-style:         disc inside none;
    padding:            0 0 0 0.1em;
}

.tip{
    position:           absolute;
    top:                10px;
    left:               -18px;
    background:         url(../img/myc.png) no-repeat 0 -1812px;
    width:              21px;
    height:             30px;
}

/************************************************************************
   End of Not Found
************************************************************************/

/************************************************************************
    One Box Search
************************************************************************/

