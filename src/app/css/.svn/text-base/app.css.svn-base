    END OF UH2
************************************************************************/

/************************************************************************
    End of One Box Search
************************************************************************/

/************************************************************************
    Loader
************************************************************************/

#myc-loader-container {
    display:                none;
    left:                   0;
    margin:                 0;
    padding:                0;
    position:               fixed;
    text-align:             left;
    top:                    0;
    visibility:             hidden;
    z-index:                10001;
    background:             transparent;
}

#myc-loader-overlay {
    height:                 100%;
    position:               relative;
    width:                  100%;
    background:             #000;
    opacity:                0;
}

/************************************************************************
    End of Loader
************************************************************************/

/************************************************************************
    Actions Form
************************************************************************/

.actions-form {
    background:        #FFFFFF;
    position:          relative;
    font-size:         12px;
    border:            1px solid #bcbcbc;
    display:           block;
    padding:           15px 27px;
    margin:            -1px 0 0 0;
}

.actions-form .close {
    top:                12px;
    right:              12px;
}

ul.actions{
    padding:            0.769em 0 0 0;
}

.actions li.first-child,
.actions li.first-child button{
    padding-left:       0;
}

.actions li.last-child{
    border:             none;
    padding-right:      0;
}

.actions li button{
    padding:            0;
    color:              #324FE1;
    font-size:          11px;
    line-height:        13px;
}

.actions li{
    padding:            0 0.692em;
    border-right:       1px solid #FFF;
    display:            inline;
    float:              left;
    vertical-align:     middle;
    position:           relative;
}

.actions li.active{
    border-top:         1px solid #bcbcbc;
    border-bottom:      1px solid #ffffff;
    z-index:            1;
    background:         #ffffff;
    margin-top:         -1px;
    padding:            0 0.692em 0.3em;
}

.actions li.border{
    border-right:       1px solid #bcbcbc;
    border-left:        1px solid #bcbcbc;  
}

.actions li.no-left-border{
    border-left:        none;
}

.actions li.no-right-border{
    border-right:       none;
}

.actions .last-child.active{
    border-right:       1px solid #bcbcbc;
}

.actions button:focus,
.actions input[type="button"]:focus{
    outline:            none;
}

/************************************************************************
    End of Actions Form
************************************************************************/

/************************************************************************
    Context Menu
************************************************************************/

.context-menu.menu-container {
    border-radius:          2px;
    -moz-box-shadow:        0 1px 6px #929292;
    -webkit-box-shadow:     0 1px 6px #929292;
    box-shadow:             0 1px 6px #929292;
    position:               absolute;
    background:             #FFFFFF;
    background:             rgba(255, 255, 255, .9);
    z-index:                101;
}

.menu-container .menu-list{
    padding-top:            11px;
    padding-bottom:         11px;
}

.menu-container .menu-list .menu-item{
    padding:                4px 8px;
    display:                block;
    cursor:                 pointer;
    -webkit-touch-callout:  none;
    -webkit-user-select:    none;
    -khtml-user-select:     none;
    -moz-user-select:       none;
    -ms-user-select:        none;
    user-select:            none;
}

.menu-container .menu-list .menu-item button{
    color:                  #333333;
    font-size:              14px;
    padding:                0;
}

.menu-container .menu-list .menu-item button:hover{
    text-decoration:        none;
}

.menu-container .menu-list .menu-item.center-here,
.menu-container .menu-list .menu-item.directions-to{
    border-bottom:          1px solid #BCBCBC;
}

.menu-container .menu-list .menu-item.disabled,
.menu-container .menu-list .menu-item.disabled button{
    background:             #FFFFFF !important;
    cursor:                 default;
    color:                  #AAAAAA;
}

.menu-container .menu-list .menu-item.hover{
    background:             #BFDAFF;
}

.context-menu .close{
    top:                    5px;
    right:                  2px;
}

/************************************************************************
    End of Context Menu
************************************************************************/

/************************************************************************
    Fader
************************************************************************/
#myc-fader-container{
    display:            none;
    left:               0;
    margin:             0;
    padding:            0;
    position:           fixed;
    text-align:         left;
    top:                0;
    visibility:         hidden;
    z-index:            10000;
}

#myc-fader-overlay {
    height:             100%;
    position:           relative;
    width:              100%;
    background:         #fff;
    /* Firefox */
    -moz-transition-property: opacity;
    -moz-transition-duration: 2s;
    -moz-transition-delay: 1s;
    /* WebKit */
    -webkit-transition-property: opacity;
    -webkit-transition-duration: 2s;
    -webkit-transition-delay: 1s;
    /* Opera */
    -o-transition-property: opacity;
    -o-transition-duration: 2s;
    -o-transition-delay: 1s;

    -ms-transition-property: opacity;
    -ms-transition-duration: 2s;
    -ms-transition-delay: 1s;
    /* Standard */
    transition-property: opacity;
    transition-duration: 2s;
    transition-delay: 1s;
    opacity:            0;
    filter:alpha(opacity=0);
}

#myc-fader-overlay.anim{
    /* Firefox */
    -moz-transition-property: opacity;
    -moz-transition-duration: 2s;
    -moz-transition-delay: 1s;
    /* WebKit */
    -webkit-transition-property: opacity;
    -webkit-transition-duration: 2s;
    -webkit-transition-delay: 1s;
    /* Opera */
    -o-transition-property: opacity;
    -o-transition-duration: 2s;
    -o-transition-delay: 1s;

    -ms-transition-property: opacity;
    -ms-transition-duration: 2s;
    -ms-transition-delay: 1s;
    /* Standard */
    transition-property: opacity;
    transition-duration: 2s;
    transition-delay: 1s;
    opacity:            .5;
    filter:alpha(opacity=50);
}
/************************************************************************
    End of Fader
************************************************************************/

/* YUI CSS Detection Stamp */
#yui3-css-stamp.myc-app-css { display: none; }