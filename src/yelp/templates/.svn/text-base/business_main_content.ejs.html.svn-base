<% 
    var cba = {
        0    : 'This business is closed',
        50   : 'This business may be closed'
    };
%>
<div class="distance">
	<span class="imperial"><%= b.get('distance') %> mi</span>
</div>
<% 
if (b.get('listing_status') && b.get('listing_status')['@score'] && b.get('listing_status')['@score'] != 100) {
%>
	<p class="closed_business">
		<span><%= cba[b.get('listing_status')['@score']] %></span>
	</p>
<% }
if (b.get('rating') && !isNaN(b.get('rating'))) {
%>
<p class="rating-review">
	<a target="_blank" href="<%= b.get('readreviewsurl') %>">
		<span class="rating-static f-left rating-<%= (b.get('rating')*10) %>">
			<%= b.get('rating') %> out of 5 stars
		</span>
		<% if (b.get('nreview')) {%>
		(<span class="review">based on </span><%= b.get('nreview') %> reviews)
		<% } %>
	</a>
</p>
<% } if (b.get('website') && b.get('website')['$']) {%>
<p class="web">
	<a href="<%= b.get('website')['$'] %>" target="_blank" class="url" title="<%= b.get('websitelabel') || b.get('website')['@label'] %>">
		<%= b.get('webLabel') %>
	</a>
</p>
<% } if (b.address()) {%>
<p class="adr"><%= b.address() %></p>
<% } %>